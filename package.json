{
  "name": "gelatin-ui",
  "version": "0.1.3",
  "private": false,
  "main": "./dist/gelatin-ui.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --target lib --name gelatin-ui ./packages/index.js",
    "publish-dev": "lint-staged && npm run build && git add . && git cz && git push",
    "publish-master": "standard-version && git push",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "axios": "^0.18.0",
    "iselement": "^1.1.4",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.0",
    "normalize.css": "^8.0.1",
    "qrcode": "^1.3.2",
    "vue": "^2.5.17",
    "vue-click-outside": "^1.0.7",
    "vue-i18n": "^8.3.0",
    "vue2-transitions": "^0.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-prettier": "^3.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "standard-version": "^4.4.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^1.0.0-alpha.27",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
