{
  "name": "gelatin-ui",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/gelatin-ui.umd.min.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src packages --fix",
    "build": "vue-cli-service build --target lib --name gelatin-ui packages/index.js",
    "publish-dev": "yarn run lint && yarn run build && git add . && git cz",
    "publish-master": "standard-version && git cz && git push",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "sh deploy.sh",
    "report": "vue-cli-service build packages/index.js --report"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "axios": "^0.18.0",
    "iselement": "^1.1.4",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.0",
    "normalize.css": "^8.0.1",
    "qrcode": "^1.3.2",
    "v-click-outside": "^2.0.2",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/runtime-corejs2": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "commitizen": "^3.0.5",
    "cssnano": "^4.1.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "register-service-worker": "^1.5.2",
    "sass-loader": "^7.1.0",
    "standard-version": "^4.4.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^1.0.0-alpha.30",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
