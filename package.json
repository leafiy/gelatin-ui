{
  "name": "gelatin-ui",
  "version": "0.1.68",
  "private": false,
  "main": "./dist/gelatin-ui.umd.min.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src packages --fix",
    "build": "vue-cli-service build --target lib --name gelatin-ui packages/index.js",
    "publish-dev": "yarn run lint && git add . && git cz",
    "publish-master": "yarn build && git add .  && git commit -m 'update build packages' && yarn release",
    "sync": "git add . && git commit -m 'dev in progress' && git push",
    "release": "standard-version -r patch && git push -u origin master",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "sh deploy.sh",
    "report": "vue-cli-service build packages/index.js --report"
  },
  "dependencies": {
    "autosize": "^4.0.2",
    "axios": "^0.18.0",
    "buxton": "^0.2.24",
    "lazyload": "^2.0.0-rc.2",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.1",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.15.0",
    "qrcode": "^1.3.3",
    "resize-sensor": "^0.0.6",
    "tua-body-scroll-lock": "^0.1.0",
    "vue": "^2.6.10",
    "vue-functional-data-merge": "^2.0.7",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-resize": "^0.4.5",
    "vue-resize-directive": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime-corejs2": "^7.4.3",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-istanbul": "^5.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "commitizen": "^3.0.7",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "register-service-worker": "^1.6.2",
    "sass-loader": "^7.1.0",
    "standard-version": "^5.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.0-alpha.47",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
